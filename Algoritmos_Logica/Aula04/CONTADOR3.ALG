Algoritmo "Contador3"

Var
   contador, numero, salto: inteiro


Inicio
   contador <- 0
   escreva("Até qual números você quer contar? ")
   leia(numero)

   escreva("Você quer contar saltando de quanto em quanto? ")
   leia(salto)

   enquanto (contador <= numero) faca
      escreval(contador)
      contador <- contador + salto
   fimenquanto
   escreval("Terminei de contar!")




Fimalgoritmo